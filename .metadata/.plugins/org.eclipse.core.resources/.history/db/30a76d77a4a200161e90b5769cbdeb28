package LinkedLists.linkedListCycle141;

import LinkedLists.ListNode;

public class linkedListCycle141 {
	
	public static boolean hasCycle(ListNode head){
		if(head == null || head.next != null){
			return false;
		}
		
		
		ListNode slow = head;
		ListNode fast = head;
		
		while(slow != null && slow.next != null){
			slow = slow.next;
			fast = fast.next.next;
			
			if(slow == fast){
				return true;
			}
		}
	}

}
