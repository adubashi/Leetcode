package Bloomberg.removeDuplicatesFromSortedArray26;

public class removeDuplicatesFromSortedArray26 {
	
	public static int removeDuplicatesNaive(int[] A) {
		int j = 0;
		int i = 1;
		
		
		while(i < A.length){
			if(A[i] == A[j]){
				//Increment the counter
				i++;
			} else {
				A[j] = A[i];
			}
		}
		
		
	}

}
