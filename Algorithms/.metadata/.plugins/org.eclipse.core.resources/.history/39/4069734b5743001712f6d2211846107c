
public class RabinKarp {
	
	private String pat;
	private long patHash;
	private int M;
	private long Q;
	//Alphabet size
	private int R = 256;
	private long RM;          // R^(M-1) % Q         
	
	public RabinKarp(String pat){
		
	}
	
	
	
	private long hash(String key, int M){
		long h = 0;
		for(int i = 0; i < M; i++){
			h = (R * h + key.charAt(i)) % M;
		}
	
		return h;
	}

}
